<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 设置背景色（白色） -->

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8dip" />

            <gradient
                android:endColor="@color/progress_bar_normal"
                android:startColor="@color/progress_bar_normal" />
        </shape>
    </item>
    <!-- 设置进度条颜色(网页加载进度条) -->

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8dip" />

                <gradient
                    android:endColor="@color/app_top_bg"
                    android:startColor="@color/app_top_bg" />
            </shape>
        </clip>
    </item>

</layer-list>